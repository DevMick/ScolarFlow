{
  "name": "edustats",
  "version": "1.0.0",
  "description": "Application web éducative pour la gestion des évaluations scolaires",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:build": "cd apps/api && pnpm test:build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:setup": "cd apps/api && npx prisma generate && npx prisma db push",
    "db:seed": "cd apps/api && npx prisma db seed",
    "clean": "turbo run clean",
    "api": "cd apps/api && pnpm dev",
    "web": "cd apps/web && pnpm dev"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "prettier": "^3.1.0",
    "turbo": "^1.10.16",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
